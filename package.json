  "repository": {
    "type": "git",
    "url": "https://github.com/terraform-cdk-providers/cdktf-provider-azurerm.git"
  },
  "scripts": {
    "clobber": "npx projen clobber",
    "bump": "npx projen bump",
    "unbump": "npx projen unbump",
